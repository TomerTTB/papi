                  "tableResources": {
                    "type": "object",
                    "properties": {
                      "copyGlass": {
                        "type": "object",
                        "properties": {
                          "sph": { "type": "string" },
                          "cyl": { "type": "string" },
                          "axis": { "type": "string" },
                          "right": { "type": "string" },
                          "left": { "type": "string" }
                        }
                      }
                    }
                  },
                  "type": { "type": "string" },
                  "id": { "type": "string" }
                }
              },
              "bottom_section_view": {
                "type": "object",
                "required": ["type", "id"],
                "properties": {
                  "vi_image_resource": {
                    "type": "object",
