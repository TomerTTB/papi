                      "type": { "type": "string" },
                      "cdnPath": { "type": ["string", "null"] }
                    }
                  },
                  "type": { "type": "string" },
                  "id": { "type": "string" }
                }
              },
              "mixpanelEvent": { "type": ["string", "null"] },
              "prescription": { "type": ["object", "null"] },
              "extraAssets": { "type": ["object", "null"] },
