      "type": "object",
      "properties": {
        "type": { "type": "string" },
        "token": { "type": "string" }
      }
    },
    "analytics": { 
      "type": "object",
      "properties": {
        "type": { "type": "string" },
        "token": { "type": "string" }
      }
    },
    "azure_config": { 
      "type": "object",
      "properties": {
        "speech_recognition_token": { "type": "string" }
      }
    },
    "campaignId": { "type": "string" },
    "campaign_id": { "type": "string" },
    "flow_id": { "type": ["string", "null"] },
    "analytics_event": {
      "type": "object",
      "required": ["id"],
      "properties": {
        "eventName": { "type": ["string", "null"] },
        "description": { "type": "string" },
        "id": { "type": "string" }
      }
    },
    "offline_pages": { "type": "object" },
    "language_dictionary": { "type": "object" },
    "theme": {
      "type": "object",
      "properties": {
        "_id": { "type": "object" },
        "theme_name": { "type": "string" },
        "version": { "type": "number" },
        "base_color": { "type": "string" },
        "brand_color": { "type": "string" },
        "font_family": { "type": "string" },
        "button_override_color": { "type": "boolean" },
        "use_theme_colors": { "type": "boolean" },
        "logo_image_name": { "type": "string" },
        "light_color": { "type": "string" },
        "dark_color": { "type": "string" },
        "darkest_color": { "type": "string" }
      }
    },
    "coralogix_config": {
      "type": "object",
      "properties": {
        "secret_key": { "type": "string" },
        "app_name": { "type": "string" },
        "active": { "type": "boolean" },
        "log_level": { "type": "string" },
        "log_record_times": { "type": ["object", "null"] },
        "url": { "type": "string" },
        "log_level_num": { "type": "number" }
      }
    },
    "native-flows": {
      "type": "object",
      "additionalProperties": true
    },
    "native-data": {
      "type": "object",
      "additionalProperties": true
