      "additionalProperties": true,
      "properties": {
        "accuracy": { "type": "boolean" },
        "feedback": { "type": "boolean" },
        "flow": { "type": "string" },
        "first_flow": { "type": "string" },
        "flows": {
          "type": "array",
          "items": { "type": "string" }
        },
        "webappFlows": {
          "type": "array",
          "items": { "type": "string" }
        },
        "authorizedFlows": {
          "type": "array",
