    "campagin_properties": {
      "type": "object",
      "required": [
        "flow",
        "first_flow",
        "flows",
        "webappFlows",
        "authorizedFlows",
        "isServerDebugMode",
        "allow_unsupported_countries",
        "campaign_assets_file_name",
        "audioFolder",
        "imageFolder",
        "videoFolder",
        "assetsGroup",
        "locale",
        "id"
