                "type": "object",
                "properties": {
                  "native_flows_path": { "type": "string" },
                  "global_flows_path": { "type": "string" },
                  "templates_path": { "type": "string" }
                }
              }
            }
          }
        }
      }
    },
    "mixpanel_id": { "type": "string" },
    "loggly_id": { "type": "string" },
    "preload_page": { "type": ["object", "null"] },
    "preload_resources": { "type": ["object", "null"] },
    "preload_page_all_flows": { "type": "object" },
    "preload_resources_all_flows": { "type": "object" },
